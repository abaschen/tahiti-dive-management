{
    "name": "Hugo",
    "image": "hugomods/hugo:exts-non-root",
    "workspaceMount": "source=${localWorkspaceFolder},target=/src,type=bind",
    "workspaceFolder": "/src",
    "settings": {
        "html.format.templating": true
    },
    "extensions": [
        "davidanson.vscode-markdownlint"
    ],
    "forwardPorts": [
        1313
    ],
    "remoteUser": "hugo"
}